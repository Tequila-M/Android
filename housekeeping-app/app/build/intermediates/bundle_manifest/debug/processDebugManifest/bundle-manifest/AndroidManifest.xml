<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wfuhui.housekeeping"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.wfuhui.housekeeping.ui.order.PayActivity"
            android:label="@string/title_activity_pay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.address.AddressAddActivity"
            android:label="@string/title_activity_address_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.order.OrderActivity"
            android:label="@string/title_activity_order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.order.ConfirmActivity"
            android:label="@string/title_activity_confirm"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.order.EvaluationActivity"
            android:label="@string/title_activity_evaluation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.address.AddressActivity"
            android:label="@string/title_activity_address"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.project.DetailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.teacher.TeacherDetailActivity"
            android:label="@string/title_activity_teacher_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.project.ListActivity"
            android:label="@string/title_activity_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.login.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.login.RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.ui.mine.UserActivity"
            android:label="@string/title_activity_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wfuhui.housekeeping.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 7.0以上摄像头调取 -->
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.wfuhui.base.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true" >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>

        <activity
            android:name="com.lljjcoder.style.citylist.CityListSelectActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing|stateUnchanged" >
        </activity>
        <activity
            android:name="com.lljjcoder.style.citythreelist.ProvinceActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing|stateUnchanged" >
        </activity>
        <activity
            android:name="com.lljjcoder.style.citythreelist.CityActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing|stateUnchanged" >
        </activity>
        <activity
            android:name="com.lljjcoder.style.citythreelist.AreaActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing|stateUnchanged" >
        </activity>

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.wfuhui.housekeeping.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>